<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="be.kuleuven.dbproject.controller.GameController" fx:id="game">
    <children>
        <VBox spacing="5" layoutX="14.0" alignment="TOP_CENTER">
            <HBox spacing="5" alignment="CENTER">
                <Text layoutX="19.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="search:" wrappingWidth="56"/>
                <TextField fx:id="autoCompleteSearch" prefWidth="351.0"/>
                <Button fx:id="gameSearchBtn" text="search" prefHeight="27.0" prefWidth="151.0" style="-fx-background-color: #4f46e5; -fx-text-fill: white;"/>
            </HBox>
            <TableView fx:id="tblGames" prefHeight="246.0" prefWidth="567.0"/>
            <HBox spacing="5" alignment="CENTER">
                <Button fx:id="gamesAdd" text="go to add game" style="-fx-background-color: #4f46e5; -fx-text-fill: white;"/>
                <Button fx:id="refreshBtn" text="refresh" style="-fx-background-color: #4f46e5; -fx-text-fill: white;"/>
            </HBox>
        </VBox>
    </children>
</AnchorPane>